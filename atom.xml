<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Kelly McLaughlin</title>
 <link href="http://kelly-mclaughlin.com/atom.xml" rel="self"/>
 <link href="http://kelly-mclaughlin.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://kelly-mclaughlin.com/</id>
 <author>
   <name>Kelly McLaughlin</name>
   <email>mclaughlin77@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://kelly-mclaughlin.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://kelly-mclaughlin.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>